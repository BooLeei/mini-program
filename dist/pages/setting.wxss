@import "./../components/toast.wxss";
page{background-color:#f2f2f5;display:flex;flex-direction:column}.list-wrap{width:100%;flex-direction:column;font-weight:500;font-size:26rpx;margin:20rpx 0;background-color:#fff}.list-item{width:100%;height:100rpx;padding:0 40rpx;border-bottom:1px solid #f2f2f5;justify-content:space-between;align-items:center;color:#707070}.list-item icon{width:20rpx;height:20rpx;border-top:2px solid #aaa;border-right:2px solid #aaa;transform:rotate(45deg)}.list-button{display:flex;width:100%;height:100rpx;padding:0 40rpx;margin:0;border:0;border-bottom:1px solid #f2f2f5;background-color:#fff;font-size:26rpx;color:#707070;align-items:center;justify-content:space-between;box-sizing:border-box;border-radius:0;position:static}.list-button icon{width:20rpx;height:20rpx;border-top:2px solid #aaa;border-right:2px solid #aaa;transform:rotate(45deg)}.list-footer{width:100$;height:100rpx;color:#ff4d4d;align-items:center;justify-content:center;font-weight:bold;background-color:#fff;font-size:28rpx}.list-footer icon{width:30rpx;height:30rpx;background-color:#ff4d4d;margin-right:10rpx}