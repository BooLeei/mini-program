page{background-color:#f2f2f5;display:flex;flex-direction:column}.list-wrap{width:100%;flex-direction:column;font-weight:500;font-size:26rpx;margin:20rpx 0;background-color:#fff}.list-item{width:100%;height:100rpx;padding:0 40rpx;border-bottom:1px solid #f2f2f5;justify-content:space-between;align-items:center;color:#707070;position:relative}.list-item icon{width:20rpx;height:20rpx;border-top:2px solid #aaa;border-right:2px solid #aaa;transform:rotate(45deg)}.list-item .special-icon{width:20rpx;height:20rpx;position:absolute;top:50%;right:40rpx;transform:translateY(-50%) rotate(45deg)}