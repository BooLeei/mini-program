
<view class="top">
    <image bindtap="toLogin" src="{{hasLogin?headerImageFull:'../image/uploadHeaderPic.png'}}"/>
    <view bindtap="toLogin">{{hasLogin?nickname:'登录/注册'}}
        <icon class="man"  wx:if="{{hasLogin&&sex==0}}"/>
        <icon class="woman"  wx:if="{{hasLogin&&sex==1}}"/>
    </view>
    <icon bindtap="slide" class="top-right-arrow"/>
</view>
<view class="list-wrap">
    <view bindtap="progess" class="list-item">
        <icon/>求职进度<icon class="list-right-arrow"/>
    </view>
    <view bindtap="resume" class="list-item">
        <icon/>我的简历<icon class="list-right-arrow"/>
    </view>
    <view bindtap="collect" class="list-item">
        <icon/>收藏职位<icon class="list-right-arrow"/>
    </view>
    <view bindtap="follow" class="list-item">
        <icon/>关注公司<icon class="list-right-arrow"/>
    </view>
    <view bindtap="setting" class="list-item">
        <icon/>我的设置<icon class="list-right-arrow"/>
    </view>
    <view bindtap="simpleResume" class="list-item">
        <icon/>简版简历测试入口<icon class="list-right-arrow"/>
    </view>
</view>

<view class="toast-wrap" hidden="{{$toast$hide}}" style="top:{{$toast$top}}px">{{$toast$content}}</view>
